@model ObligatorioProgramacion3.Models.Reserva

@{
    ViewData["Title"] = "Details";
}
<head>
    <link rel="stylesheet" href="~/css/StyleCrudeDetails.css" />
</head>


<div class="details-container">

    <h1 class="text-center" color="white">Detalles</h1>
    <dl >
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaReserva)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaReserva)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cliente.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdRestauranteNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdRestauranteNavigation.Dirección)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Mesa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Mesa.Estado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Usuario.Id)
        </dd>
    </dl>
    <div class="button-group">
        <a class="button-link" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>
        <a class="button-link" asp-action="Index">Back to List</a>
    </div>
</div>
