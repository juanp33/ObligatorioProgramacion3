@model ObligatorioProgramacion3.Models.Reserva

@{
    ViewData["Title"] = "Delete";
}

<head>
    <link rel="stylesheet" href="~/css/StyleCrudeCreate.css" asp-append-version="true" />
</head>
<div>
<dl class="row">
    <h3>Estas seguro que deseas borrar esto?</h3>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.FechaReserva)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.FechaReserva)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Estado)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Estado)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Cliente)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Cliente.Email)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.IdRestauranteNavigation)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.IdRestauranteNavigation.Dirección)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Mesa)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Mesa.Estado)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Usuario)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Usuario.Id)
    </dd>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="button-link" asp-action="Index">Volver</a>
    </form>
</dl>
    
   
</div>
