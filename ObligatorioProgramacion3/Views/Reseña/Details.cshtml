@model ObligatorioProgramacion3.Models.Reseña

@{
    ViewData["Title"] = "Details";
}


<head>
    <link rel="stylesheet" href="~/css/StyleCrudeDetails.css" />
</head>

<div class="details-container">

    <h1 class="text-center" color="white">Detalles</h1>
    <dl >
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Puntaje)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Puntaje)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Comentario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Comentario)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaReseña)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaReseña)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Restaurante)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Restaurante.Id)
        </dd>
    </dl>
    <div class="button-group">
        <a class="button-link" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>
        <a class="button-link" asp-action="Index">Back to List</a>
    </div>
</div>

